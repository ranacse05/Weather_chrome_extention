{
    "manifest_version": 3,
    "name": "Weather",
    "version": "1.0",
  
    "description": "Shows weater info of saved location on pop-up",
  
    "icons": {
        "16": "/icons/16.png",
        "32": "/icons/32.png",
        "48": "/icons/48.png",
        "128": "/icons/128.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.mozilla.org/*"],
        "js": ["script.js"]
      }
    ]
  
  }